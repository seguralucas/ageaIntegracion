package exit.services.parser;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class ApuntadorDeEntidad {
	private List<String> entidades;
	private static ApuntadorDeEntidad instance;
	private ApuntadorDeEntidad(){
		String[] aux= RecuperadorPropiedadesConfiguracionGenerales.getInstance().getAction().split(",");
		entidades=Arrays.asList(aux);
	}
    public static synchronized ApuntadorDeEntidad getInstance() {
    	if(instance==null)
    		instance=new ApuntadorDeEntidad();
    	return instance;
    }}
